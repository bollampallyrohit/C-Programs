#include<stdio.h>
void swap(int *x, int *y)
{
    int i;
    i = *x;
    *x = *y;
    *y = i;
}
int main()
{
    int num1, num2;
    printf("Enter number 1: ");
    scanf("%d", &num1);
    printf("Enter number 2: ");
    scanf("%d", &num2);
    printf("Before swapping: number1: %d, number2: %d\n", num1, num2);
    swap(&num1,&num2);
    printf("After swapping: number1: %d, number2: %d\n", num1, num2);
}
